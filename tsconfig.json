{
  
  "compilerOptions": {
      "target": "ES2019",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "baseUrl": ".",
      "paths": {
        "@api": [
          "api"
        ],
        "@api/*": [
          "api/*"
        ],
        "@components": [
          "components"
        ],
        "@components/*": [
          "components/*"
        ],
        "@constants": [
          "constants"
        ],
        "@constants/*": [
          "constants/*"
        ],
        "@hooks": [
          "hooks"
        ],
        "@hooks/*": [
          "hooks/*"
        ],
        "@icons/*": [
          "components/icons/*"
        ],
        "@pages": [
          "pages"
        ],
        "@pages/*": [
          "pages/*"
        ],
        "@styles": [
          "styles"
        ],
        "@styles/*": [
          "styles/*"
        ],
        "@src/*": [
          "*"
        ],
        "@utils/*": [
          "utils/*"
        ],
        "@utils": [
          "utils"
        ],
      },
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "strictBindCallApply": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
